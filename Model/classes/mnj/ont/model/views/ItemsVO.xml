  <SQLQuery>
    <![CDATA[SELECT  PHA.SEGMENT1 PO_NUMBER,PHA.ORG_ID,PLA.ITEM_ID,
       (SELECT CONCATENATED_SEGMENTS
          FROM MTL_SYSTEM_ITEMS_KFV MSI
         WHERE MSI.INVENTORY_ITEM_ID = PLA.ITEM_ID
           AND MSI.ORGANIZATION_ID = 353) CONC_SEG,
       (SELECT MSI.DESCRIPTION
          FROM MTL_SYSTEM_ITEMS_KFV MSI
         WHERE MSI.INVENTORY_ITEM_ID = PLA.ITEM_ID
           AND MSI.ORGANIZATION_ID = 353) ITEM_DESC,
       PLA.UNIT_PRICE,PLA.QUANTITY,PLLA.NEED_BY_DATE ,

        (  SELECT CUSTOMER_NAME FROM AR_CUSTOMERS AC WHERE AC.CUSTOMER_NAME_PHONETIC IN ('UM', 'EM') AND AC.STATUS = 'A'  AND  AC.CUSTOMER_ID = 
         (SELECT H.BUYER_ID FROM XX_OM_POC_H_T H  WHERE H.POC_ID = APPS.PO_LINE_POC_ID_F(PLA.PO_LINE_ID))
            
        ) BUYER_NAME ,
      
      ( SELECT    H.SEASON FROM XX_OM_POC_H_T H  WHERE H.POC_ID =   APPS.PO_LINE_POC_ID_F(PLA.PO_LINE_ID)) SEASON,
      
       ( SELECT    H.STYLE FROM XX_OM_POC_H_T H  WHERE H.POC_ID =   APPS.PO_LINE_POC_ID_F(PLA.PO_LINE_ID)) STYLE,
               
        PLLA.NOTE_TO_RECEIVER STYLE_SEASON,
          TRIM (SUBSTR(MRB.ATTRIBUTE14,INSTR(MRB.ATTRIBUTE14,'/',-1)+1))GARMENT_QTY ,
      
        APPS.PO_LINE_POC_ID_F(PLA.PO_LINE_ID) POC_ID 
    
        FROM PO_HEADERS_ALL PHA, PO_LINES_ALL PLA,PO_LINE_LOCATIONS_ALL PLLA,
           PO_DISTRIBUTIONS_ALL PDA,
                 MNJ_REQUISITION_BOM_V MRB
         WHERE PHA.PO_HEADER_ID = PLA.PO_HEADER_ID       
          AND PLLA.PO_HEADER_ID(+)=PLA.PO_HEADER_ID
          AND PLLA.PO_LINE_ID(+)=PLA.PO_LINE_ID
             AND PDA.PO_HEADER_ID = PHA.PO_HEADER_ID
             AND PDA.PO_LINE_ID   = PLA.PO_LINE_ID
             AND PDA.REQ_DISTRIBUTION_ID = MRB.DISTRIBUTION_ID(+)
        AND (PLLA.CANCEL_FLAG IS NULL OR PLLA.CANCEL_FLAG='N')]]>
  </SQLQuery>

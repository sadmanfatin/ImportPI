  <SQLQuery>
    <![CDATA[SELECT /* MSI.PRIMARY_UOM_CODE,*/
  /* (SELECT SUM(IIP.ATTRIBUTE4) 
        FROM IEDOC_IMP_PI_ITEM_DETAIL IIP,mtl_system_items m
        WHERE IIP.ITEM_ID=M.INVENTORY_ITEM_ID
        AND M.ORGANIZATION_ID=353
        AND IIP.IMP_PI_DETAIL_ID=ImpPiItemEO.IMP_PI_DETAIL_ID
        AND MSI.PRIMARY_UOM_CODE=M.PRIMARY_UOM_CODE
        AND MSI.SEGMENT1=M.SEGMENT1  ) TOT_QTY,*/
       (SELECT SUM(IIP.ATTRIBUTE4) 
        FROM IEDOC_IMP_PI_ITEM_DETAIL IIP,mtl_system_items m
        WHERE IIP.ITEM_ID=M.INVENTORY_ITEM_ID
        AND M.ORGANIZATION_ID=353
        AND IIP.IMP_PI_DETAIL_ID=ImpPiItemEO.IMP_PI_DETAIL_ID
        AND IIP.ITEM_ID=ImpPiItemEO.ITEM_ID ) TOT_QTY,
       ImpPiItemEO.IMP_PI_ITEM_DET_ID, 
       ImpPiItemEO.IMP_PI_DETAIL_ID, 
       ImpPiItemEO.ITEM_ID, 
       ImpPiItemEO.HS_CODE, 
       ImpPiItemEO.LAST_UPDATE_LOGIN, 
       ImpPiItemEO.LAST_UPDATE_DATE, 
       ImpPiItemEO.LAST_UPDATED_BY, 
       ImpPiItemEO.CREATION_DATE, 
       ImpPiItemEO.CREATED_BY, 
       ImpPiItemEO.ATTRIBUTE1, 
       ImpPiItemEO.ATTRIBUTE2, 
       ImpPiItemEO.ATTRIBUTE3, 
       ImpPiItemEO.ATTRIBUTE4, 
       ImpPiItemEO.ATTRIBUTE5, 
       ImpPiItemEO.ATTRIBUTE6, 
       ImpPiItemEO.ATTRIBUTE7, 
       ImpPiItemEO.ATTRIBUTE8, 
       ImpPiItemEO.ATTRIBUTE9, 
       ImpPiItemEO.ATTRIBUTE10, 
       ImpPiItemEO.ATTRIBUTE11, 
       ImpPiItemEO.ATTRIBUTE12, 
       ImpPiItemEO.ATTRIBUTE13, 
       ImpPiItemEO.ATTRIBUTE14, 
       ImpPiItemEO.ATTRIBUTE15, 
       ImpPiItemEO.ATTRIBUTE16, 
       ImpPiItemEO.ATTRIBUTE17, 
       ImpPiItemEO.ATTRIBUTE18, 
       ImpPiItemEO.ATTRIBUTE19, 
       ImpPiItemEO.ATTRIBUTE20, 
       ImpPiItemEO.ATTRIBUTE21, 
       ImpPiItemEO.ATTRIBUTE22, 
       ImpPiItemEO.ATTRIBUTE23, 
       ImpPiItemEO.ATTRIBUTE24, 
       ImpPiItemEO.ATTRIBUTE25, 
       ImpPiItemEO.ATTRIBUTE26, 
       ImpPiItemEO.ATTRIBUTE27, 
       ImpPiItemEO.ATTRIBUTE28, 
       ImpPiItemEO.ATTRIBUTE29, 
       ImpPiItemEO.ATTRIBUTE30, 
       ImpPiItemEO.TRANS_QTY1, 
       ImpPiItemEO.TRANS_QTY2, 
       ImpPiItemEO.TRANS_QTY3, 
       ImpPiItemEO.TRANS_QTY4, 
       ImpPiItemEO.TRANS_QTY5, 
       ImpPiItemEO.TRANS_QTY6, 
       ImpPiItemEO.TRANS_DATE1, 
       ImpPiItemEO.TRANS_DATE2, 
       ImpPiItemEO.TRANS_DATE3, 
       ImpPiItemEO.TRANS_DATE4, 
       ImpPiItemEO.TRANS_DATE5, 
       ImpPiItemEO.TRANS_DATE6,
       (SELECT PLA.UNIT_MEAS_LOOKUP_CODE 
        FROM PO_LINES_ALL PLA,PO_HEADERS_ALL PHA
        WHERE PLA.PO_HEADER_ID=PHA.PO_HEADER_ID
        AND PHA.SEGMENT1=ImpPiDetailsEO.PO_NO
        AND PHA.ORG_ID=ImpPiLinesEO.ORG_ID
        AND PLA.ITEM_ID=ImpPiItemEO.ITEM_ID
        AND ROWNUM=1) PRIMARY_UOM_CODE,
        (SELECT PLA.VENDOR_PRODUCT_NUM
           FROM PO_LINES_ALL PLA,PO_HEADERS_ALL PHA
            WHERE PLA.PO_HEADER_ID=PHA.PO_HEADER_ID
            AND PHA.SEGMENT1=ImpPiDetailsEO.PO_NO
            AND PHA.ORG_ID=ImpPiLinesEO.ORG_ID
            AND PLA.ITEM_ID=ImpPiItemEO.ITEM_ID
            AND ROWNUM=1) STYLE_NO, 
      (  select customer_name from ar_customers ac where AC.CUSTOMER_NAME_PHONETIC in ('UM', 'EM') and AC.STATUS = 'A'  AND  AC.CUSTOMER_ID = 
         (SELECT H.BUYER_ID FROM XX_OM_POC_H_T H  WHERE H.POC_ID = ImpPiItemEO.POC_ID)            
      ) BUYER ,
  
       ( SELECT H.SEASON FROM XX_OM_POC_H_T H  WHERE H.POC_ID = ImpPiItemEO.POC_ID  ) SEASON,     
       ImpPiItemEO.POC_ID

FROM IEDOC_IMP_PI_ITEM_DETAIL ImpPiItemEO, mtl_system_items msi,IEDOC_IMP_PI_DETAILS ImpPiDetailsEO,IEDOC_IMP_PI_LINES ImpPiLinesEO
   WHERE ImpPiItemEO.ITEM_ID=MSI.INVENTORY_ITEM_ID
   AND ImpPiDetailsEO.IMP_PI_DETAIL_ID=ImpPiItemEO.IMP_PI_DETAIL_ID
   AND ImpPiLinesEO.IMP_PI_LINE_ID=ImpPiDetailsEO.IMP_PI_LINE_ID
   AND MSI.ORGANIZATION_ID=353]]>
  </SQLQuery>

<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SeasonVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="mnj.ont.model.lov.views.SeasonVORowImpl"
  ComponentClass="mnj.ont.model.lov.views.SeasonVOImpl"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess"/>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.views.SeasonVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT CHARACTER2 SEASON FROM QA_RESULTS QR WHERE QR.PLAN_ID = (SELECT PLAN_ID FROM QA_PLANS QP WHERE QP.NAME ='BUYER WISE DETAILS OM')
UNION ALL
SELECT FFV.FLEX_VALUE SEASON
  FROM FND_FLEX_VALUE_SETS FFVS, FND_FLEX_VALUES FFV, FND_FLEX_VALUES_TL FFVT
 WHERE FFVS.FLEX_VALUE_SET_ID = FFV.FLEX_VALUE_SET_ID
   AND FFV.FLEX_VALUE_ID = FFVT.FLEX_VALUE_ID
   AND ENABLED_FLAG = 'Y'
   AND FLEX_VALUE_SET_NAME LIKE 'OM_PI_SEASON'
ORDER BY SEASON]]>
  </SQLQuery>
  <ViewAttribute
    Name="Season"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SEASON"
    Expression="SEASON"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="150"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mnj.ont.model.lov.views.SeasonVO.Season_LABEL"/>
        <DISPLAYWIDTH
          Value="30"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>

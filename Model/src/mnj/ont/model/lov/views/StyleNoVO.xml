  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.ont.model.lov.views.StyleNoVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT PACKING_INSTRUCTIONS STYLE_NO FROM OE_ORDER_HEADERS_ALL
UNION ALL
SELECT FFV.FLEX_VALUE STYLE_NO
  FROM FND_FLEX_VALUE_SETS FFVS, FND_FLEX_VALUES FFV, FND_FLEX_VALUES_TL FFVT
 WHERE FFVS.FLEX_VALUE_SET_ID = FFV.FLEX_VALUE_SET_ID
   AND FFV.FLEX_VALUE_ID = FFVT.FLEX_VALUE_ID
   AND ENABLED_FLAG = 'Y'
   AND FLEX_VALUE_SET_NAME LIKE 'OM_PI_STYLE_NUMBER'
ORDER BY STYLE_NO]]>
  </SQLQuery>

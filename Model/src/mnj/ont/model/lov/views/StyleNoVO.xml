      <Attr Name="_DisplaySize" Value="2000"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mnj.ont.model.lov.views.StyleNoVO.StyleNo_LABEL"/>
        <DISPLAYWIDTH
          Value="30"/>
